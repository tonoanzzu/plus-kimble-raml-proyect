#%RAML 1.0
types:
  resource:
    type:object
    description: Entity that represent an resource
    properties:
      id:
        type: integer
        description: identify of the resource
        required: true
      name:
        type: string
        description: identify the name of the resource
        required: true
      last_name:
        type: string
        description: identify the last name of the resource
        required: true
      second_last_name:
        type: string
        description: identify the second last name of the resource
        required: true
      fkResourceStatus:
        type: integer
        description: identify the state of the resource
        required: true
      letters_gft: 
        type: string
        description: identify the letters of the resource
        required: true
      starting_date:
        type: string
        description: identify the admission date of the resource
        required: true
      end_date:
        type: string
        description: identify the last date of the resource
        required: true
      comentary:
        type: string
        description: identify the text referring to a resource
        required: true